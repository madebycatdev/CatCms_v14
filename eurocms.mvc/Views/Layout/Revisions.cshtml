@model IList<EuroCMS.Admin.entity.cms_asp_admin_select_template_revisions_Result>
 
<!-- BEGIN SAMPLE TABLE PORTLET-->
<div class="row-fluid">
    <div class="span12">
        <div class="portlet box grey">
            <div class="portlet-title">
                <div class="caption"><i class="icon-reorder"></i>Revisions List</div>
            </div>
            <div class="portlet-body">
                <table class="table table-striped table-hover">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Created By</th>
                            <th>Actions </th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.Count > 0)
                        { 
                            foreach (var _layout in Model)
                            { 
                                <tr>
                                    <td> @_layout.history_id </td>
                                    <td> @_layout.created by @_layout.publisher_name </td>
                                    <td> 
                                        <a href="@Url.Action("Edit", new { id= @_layout.template_id, RevisionId = _layout.history_id })" class="btn icn-only black" title="View This Revisions"><i class="icon-eye-open"></i></a>
                                        @*<a href="@Url.Action("Delete", new { id= @_layout.template_id})" class="btn icn-only black" onclick="return deleteObject(this);" data-message="Are you sure want to delete this layout?"><i class="icon-trash"></i></a>*@
                                    </td>
                                </tr>
                            }
                        }
                        else
                        {
                            <tr><td colspan="4">No Result Found!</td></tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<!-- END SAMPLE TABLE PORTLET-->

