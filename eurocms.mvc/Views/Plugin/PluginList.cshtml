@model List<eurocms.entity.cms_asp_select_plugins_Result>
@{ Layout = null; }
<div id='objectList'>
    @if(Model.Count > 0)
    {
        int i = 1;
        <ul>
        @foreach (var plugin in Model)
        { 
            <li>
                <a onclick="return iPl(@plugin.plugin_id,'@plugin.plugin_name'),false;" href='javascript:void(0);'><img src='@Url.Content("~/Content/img/icon_article.gif")' />@plugin.plugin_name</a>
            </li>            
            i++;
        }
        </ul>
    }
    else
    {
        <p>No Plugin Found!</p>
    }
</div>